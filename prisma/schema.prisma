

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}


model Example {
  id              Int       @default(autoincrement()) @id
  createdAt       DateTime  @default(now())
  updatedAt       DateTime  @updatedAt
}

model userModel {
  id              Int       @default(autoincrement()) @id
  username        String  
  updatedAt       DateTime  @updatedAt
}


model gamesModel {
  id              Int       @default(autoincrement()) @id
  createdAt       DateTime  @default(now())
  updatedAt       DateTime  @updatedAt
  Title           String 
  Status          Boolean
  reviewDesc      String?
  starRating      Int
}